{
  "name": "Last Resort",
  "version": "0.6",
  "description": "Override artist images on Last.fm!",
  "permissions": ["activeTab", "declarativeContent", "storage", "https://*.last.fm/*", "webNavigation"],
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Click to replace images on this page."
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2
}
